datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model PostgreSqlRecord {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}

model KafkaMessage {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  offset    Int?
  partition Int?
  payload   Json?
  topic     String?
  updatedAt DateTime @updatedAt
}
